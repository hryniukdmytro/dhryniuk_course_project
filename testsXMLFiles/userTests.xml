<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="User Tests">
    <test name="User API Tests">
        <classes>
            <class name="course_project.runners.apiRunners.userFeatures.UserAPITestRunner"/>
        </classes>
    </test>
    <test name="[Chrome] User UI Tests">
        <classes>
            <class name="course_project.runners.uiRunners.userFeatures.UserTestRunnerChrome"/>
        </classes>
    </test>
    <test name="[Chrome Headless] User UI Tests">
        <classes>
            <class name="course_project.runners.uiRunners.userFeatures.UserTestRunnerChromeHeadless"/>
        </classes>
    </test>
    <test name="[Firefox] User UI Tests">
        <classes>
            <class name="course_project.runners.uiRunners.userFeatures.UserTestRunnerFirefox"/>
        </classes>
    </test>
    <test name="[Chrome] User Rerun failed UI Tests">
        <classes>
            <class name="course_project.runners.uiRunners.retryFailed.RerunTestRunnerChrome"/>
        </classes>
    </test>
    <test name="[Chrome Headless] User Rerun failed UI Tests">
        <classes>
            <class name="course_project.runners.uiRunners.retryFailed.RerunTestRunnerChromeHeadless"/>
        </classes>
    </test>
    <test name="[Firefox] User Rerun failed UI Tests">
        <classes>
            <class name="course_project.runners.uiRunners.retryFailed.RerunTestRunnerFirefox"/>
        </classes>
    </test>
</suite>
